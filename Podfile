# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'BurstChat' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BurstChat
  pod 'Firebase/Auth'
  pod 'Firebase/Core'
  pod 'GoogleSignIn'

  target 'BurstChatTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BurstChatUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
